@startuml Client Management
class Client {
    + ClientId : Guid
    + ClientName : string
    + Email : string
    + TotalRevenue : decimal
    + Status : ClientStatus
}
class Invoice {
    + InvoiceId : Guid
    + ClientId : Guid
    + Amount : decimal
    + DueDate : DateTime
    + Status : InvoiceStatus
}
@enduml
