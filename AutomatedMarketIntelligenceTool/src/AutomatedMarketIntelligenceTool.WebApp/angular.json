{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "automated-market-intelligence-tool": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "standalone": true
        }
      },
      "root": "projects/automated-market-intelligence-tool",
      "sourceRoot": "projects/automated-market-intelligence-tool/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/automated-market-intelligence-tool",
            "index": "projects/automated-market-intelligence-tool/src/index.html",
            "browser": "projects/automated-market-intelligence-tool/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/automated-market-intelligence-tool/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/automated-market-intelligence-tool/src/favicon.ico",
              "projects/automated-market-intelligence-tool/src/assets"
            ],
            "styles": [
              "@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/automated-market-intelligence-tool/src/styles/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "automated-market-intelligence-tool:build:production"
            },
            "development": {
              "buildTarget": "automated-market-intelligence-tool:build:development"
            }
          },
          "defaultConfiguration": "development"
        }
      }
    }
  }
}
