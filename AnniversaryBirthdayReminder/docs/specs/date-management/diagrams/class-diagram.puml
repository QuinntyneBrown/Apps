@startuml
!theme plain
class ImportantDate {
  - DateId: Guid
  - PersonName: string
  - DateType: enum
  - DateValue: DateTime
  - RecurrencePattern: enum
  + Create(): ImportantDate
  + Update(): void
}
class ImportantDateCreated {
  + DateId: Guid
  + PersonName: string
  + DateType: string
  + OccurredAt: DateTime
}
ImportantDate ..> ImportantDateCreated : publishes
@enduml
