@startuml Reminders - Use Case Diagram

actor "User" as user
actor "System" as system

rectangle "Reminders" {
  usecase "Configure Reminder" as UC1
  usecase "Receive Reminder" as UC2
  usecase "Snooze Reminder" as UC3
  usecase "Complete from Reminder" as UC4
  usecase "View Upcoming" as UC5
  usecase "Set Quiet Hours" as UC6
  usecase "Generate Reminders" as UC7
  usecase "Track Effectiveness" as UC8
}

user --> UC1
user --> UC3
user --> UC4
user --> UC5
user --> UC6
system --> UC2
system --> UC7
system --> UC8

@enduml
