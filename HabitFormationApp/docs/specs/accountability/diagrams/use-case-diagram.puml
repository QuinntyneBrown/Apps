@startuml Accountability - Use Case Diagram

actor "User" as user
actor "Partner" as partner

rectangle "Accountability" {
  usecase "Add Partner" as UC1
  usecase "Accept Invitation" as UC2
  usecase "View Partner Progress" as UC3
  usecase "Send Encouragement" as UC4
  usecase "Complete Check-In" as UC5
  usecase "Receive Alert" as UC6
  usecase "Remove Partner" as UC7
}

user --> UC1
user --> UC5
user --> UC7
partner --> UC2
partner --> UC3
partner --> UC4
partner --> UC6

@enduml
