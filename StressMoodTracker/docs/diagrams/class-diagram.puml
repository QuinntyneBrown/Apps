@startuml StressMoodTracker Class Diagram

class User {
  +UUID id
  +String email
  +String name
  +DateTime createdAt
}

class MoodEntry {
  +UUID id
  +UUID userId
  +Integer moodRating
  +String[] emotionTags
  +Integer intensityLevel
  +String context
  +DateTime timestamp
  --
  +calculateAverage()
}

class StressEntry {
  +UUID id
  +UUID userId
  +Integer stressRating
  +String stressType
  +String[] physicalSymptoms
  +DateTime timestamp
  --
  +triggerAlert()
}

class Trigger {
  +UUID id
  +UUID userId
  +String triggerType
  +String description
  +Integer associatedMoodStress
  +Integer frequencyCount
  +DateTime firstOccurrence
  --
  +detectPattern()
}

class JournalEntry {
  +UUID id
  +UUID userId
  +String text
  +Integer wordCount
  +Integer associatedMood
  +Integer associatedStress
  +String[] tags
  +String entryType
  +DateTime timestamp
  --
  +analyzeSentiment()
  +extractKeywords()
}

class CopingStrategy {
  +UUID id
  +UUID userId
  +String strategyType
  +Integer duration
  +Integer preInterventionMood
  +Integer postInterventionMood
  +Integer effectivenessRating
  +DateTime appliedAt
  --
  +calculateEffectiveness()
}

class WeeklyInsight {
  +UUID id
  +UUID userId
  +Date weekStart
  +Decimal averageMood
  +Decimal averageStress
  +String[] keyTriggers
  +String[] copingSuccesses
  +String[] patternsIdentified
  +DateTime generatedAt
  --
  +generate()
}

class CheckInSchedule {
  +UUID id
  +UUID userId
  +String frequency
  +Time[] scheduledTimes
  +Integer streakLength
  +Boolean isActive
  --
  +trackMissed()
  +updateStreak()
}

User "1" -- "0..*" MoodEntry : logs
User "1" -- "0..*" StressEntry : records
User "1" -- "0..*" Trigger : identifies
User "1" -- "0..*" JournalEntry : writes
User "1" -- "0..*" CopingStrategy : applies
User "1" -- "0..*" WeeklyInsight : receives
User "1" -- "0..1" CheckInSchedule : configures

MoodEntry "0..*" -- "0..*" Trigger : may relate to
StressEntry "0..*" -- "0..*" Trigger : may relate to
JournalEntry "*" -- "0..1" MoodEntry : references
JournalEntry "*" -- "0..1" StressEntry : references

@enduml
