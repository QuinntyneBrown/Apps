@startuml Date Night Idea Generator

class DateIdea <<Aggregate Root>> {
  + Id: Guid
  + Title: string
  + Category: string
  + EstimatedCost: decimal
  + Duration: int
  + Location: string
}

class DatePlan <<Entity>> {
  + IdeaId: Guid
  + ScheduledDate: DateTime
  + Reservations: string
  + Budget: decimal
}

class Experience <<Entity>> {
  + PlanId: Guid
  + CompletionDate: DateTime
  + Rating: int
  + ActualCost: decimal
  + Photos: List<string>
}

class UserPreferences <<Value Object>> {
  + LikedCategories: List<string>
  + BudgetTier: BudgetTier
  + LocationPrefs: string
}

DateIdea "1" -- "0..*" DatePlan
DatePlan "1" -- "0..1" Experience

@enduml
