@startuml TravelDestinationWishlist_ClassDiagram

package "Wishlist" {
  class WishlistDestination {
    +Guid Id
    +string LocationName
    +string Country
    +int PriorityLevel
    +string TargetSeason
    +decimal EstimatedBudget
  }
}

package "Trip Planning" {
  class Trip {
    +Guid Id
    +List<Guid> DestinationIds
    +DateTime StartDate
    +DateTime EndDate
    +decimal Budget
    +TripStatus Status
  }

  class Itinerary {
    +Guid TripId
    +List<DailyActivity> Activities
  }
}

package "Experience" {
  class AttractionVisit {
    +Guid Id
    +Guid TripId
    +string AttractionName
    +DateTime VisitDate
    +int Rating
  }

  class TravelPhoto {
    +Guid Id
    +Guid TripId
    +string PhotoUrl
    +string Location
    +DateTime PhotoDate
  }
}

package "Budget" {
  class TripBudget {
    +Guid TripId
    +decimal TotalBudget
    +Dictionary CategoryAllocations
  }

  class Expense {
    +Guid Id
    +Guid TripId
    +decimal Amount
    +string Category
    +DateTime Date
  }
}

Trip "1" -- "0..1" Itinerary
Trip "1" -- "0..*" AttractionVisit
Trip "1" -- "0..*" TravelPhoto
Trip "1" -- "0..1" TripBudget
Trip "1" -- "0..*" Expense

@enduml
