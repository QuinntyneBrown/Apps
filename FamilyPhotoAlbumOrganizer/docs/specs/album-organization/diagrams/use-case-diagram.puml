@startuml Album Organization Use Cases
!theme plain

actor "Family Member" as User

rectangle "Album Organization" {
  usecase "Create Album" as UC1
  usecase "Edit Album" as UC2
  usecase "Delete Album" as UC3
  usecase "Add Photos to Album" as UC4
  usecase "Remove Photos from Album" as UC5
  usecase "Reorganize Album Photos" as UC6
  usecase "Share Album" as UC7
  usecase "Browse Albums" as UC8
}

User --> UC1
User --> UC2
User --> UC3
User --> UC4
User --> UC5
User --> UC6
User --> UC7
User --> UC8

@enduml
