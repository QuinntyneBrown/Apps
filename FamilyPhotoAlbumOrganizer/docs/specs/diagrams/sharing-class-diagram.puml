@startuml Sharing Classes
!theme plain
class ShareLink {
  - LinkId: Guid
  - TenantId: Guid
  - ContentType: enum
  - ContentId: Guid
  - LinkUrl: string
  - ExpirationDate: DateTime
  + Generate(): ShareLink
  + Access(): Content
}
class Collaborator {
  - CollaboratorId: Guid
  - TenantId: Guid
  - AlbumId: Guid
  - PermissionLevel: enum
  + Invite(): void
  + Accept(): void
}
@enduml
