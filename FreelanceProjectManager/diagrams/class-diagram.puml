@startuml Freelance Project Manager Classes

class Client {
  +Guid Id
  +string Name
  +string Company
  +string Email
  +string PaymentTerms
  +List<Project> Projects
}

class Project {
  +Guid Id
  +Guid ClientId
  +string Name
  +string Status
  +decimal AgreedPrice
  +DateTime Deadline
  +List<TimeEntry> TimeEntries
  +List<Milestone> Milestones
}

class TimeEntry {
  +Guid Id
  +Guid ProjectId
  +DateTime Date
  +decimal Duration
  +string Description
  +bool IsBillable
  +decimal Rate
}

class Invoice {
  +Guid Id
  +Guid ClientId
  +DateTime IssueDate
  +decimal TotalAmount
  +string Status
  +List<TimeEntry> LineItems
}

class Payment {
  +Guid Id
  +Guid InvoiceId
  +DateTime PaymentDate
  +decimal Amount
}

class Milestone {
  +Guid Id
  +Guid ProjectId
  +string Name
  +DateTime CompletionDate
  +bool IsCompleted
}

Client "1" -- "*" Project
Project "1" -- "*" TimeEntry
Project "1" -- "*" Milestone
Client "1" -- "*" Invoice
Invoice "1" -- "*" Payment

@enduml
