@startuml Networking - Use Case Diagram

actor "Job Seeker" as seeker

rectangle "Networking Management" {
  usecase "Add Contact" as UC1
  usecase "Request Referral" as UC2
  usecase "Track Referral" as UC3
  usecase "Schedule Informational Interview" as UC4
  usecase "Conduct Informational Interview" as UC5
  usecase "Send Thank You Note" as UC6
  usecase "View Contact Network" as UC7
  usecase "Import LinkedIn Contacts" as UC8
}

seeker --> UC1
seeker --> UC2
seeker --> UC3
seeker --> UC4
seeker --> UC5
seeker --> UC6
seeker --> UC7
seeker --> UC8

UC2 ..> UC1 : <<include>>
UC5 ..> UC6 : <<extends>>

@enduml
