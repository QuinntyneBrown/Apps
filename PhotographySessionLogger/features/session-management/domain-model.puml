@startuml Photography Domain Model

entity "Session" as session {
    * id : Guid
    * sessionType : SessionType
    scheduledDate : DateTime
    location : string
    client : string?
    photosCaptured : int
    status : SessionStatus
}

entity "Photo" as photo {
    * id : Guid
    * sessionId : Guid
    fileName : string
    captureTime : DateTime
    camera : string
    lens : string
    settings : CameraSettings
    rating : int?
}

entity "Equipment" as equipment {
    * id : Guid
    * type : EquipmentType
    * brand : string
    * model : string
    purchaseDate : DateTime
    cost : decimal
    usageCount : int
}

entity "Client" as client {
    * id : Guid
    * name : string
    email : string
    phone : string
    bookings : int
}

entity "PhotoProject" as project {
    * id : Guid
    * name : string
    * theme : string
    targetPhotoCount : int
    status : ProjectStatus
}

session }o--o| client : "booked by"
session ||--o{ photo : "captures"
photo }o--|| equipment : "taken with"
project ||--o{ photo : "includes"

enum SessionType {
    Portrait
    Landscape
    Event
    Product
    Wedding
}

enum SessionStatus {
    Planned
    InProgress
    Completed
}

@enduml
