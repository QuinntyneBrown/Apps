@startuml
class Appointment {
    +Guid Id
    +Guid UserId
    +Guid ProviderId
    +DateTime AppointmentDate
    +AppointmentStatus Status
    +Book()
    +Complete()
    +Cancel()
}

class Provider {
    +Guid Id
    +string Name
    +string Specialty
    +string Address
}

enum AppointmentStatus {
    Scheduled
    Completed
    Cancelled
    NoShow
}

Appointment --> Provider
@enduml
